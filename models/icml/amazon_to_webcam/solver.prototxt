net: "models/icml/amazon_to_webcam/train_val.prototxt"
test_iter: 15
test_interval: 300
# lr for fine-tuning should be lower than when starting from scratch
base_lr: 0.003
lr_policy: "inv"
gamma: 0.002
power: 0.75
#lr_policy: "step"
#gamma: 0.5
#stepsize: 500
display:100
max_iter: 50000
momentum: 0.9
snapshot: 1000000
snapshot_prefix: "models/icml/amazon_to_webcam/no_mmd"
# uncomment the following to default to CPU mode solving
solver_mode: GPU
